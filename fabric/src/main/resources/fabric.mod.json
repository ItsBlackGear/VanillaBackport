{
  "schemaVersion": 1,
  "id": "vanillabackport",
  "version": "${version}",
  "name": "VanillaBackport",
  "description": "Backporting Minecraft Features to previous versions!",
  "authors": [
    "BlackGear"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/vanilla-backport"
  },
  "license": "ARR",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.blackgear.vanillabackport.core.fabric.VanillaBackportFabric"
    ],
    "fabric-datagen": [
      "com.blackgear.vanillabackport.data.DataGenerator"
    ],
    "terrablender": [
      "com.blackgear.vanillabackport.core.fabric.VanillaBackportTerrablender"
    ]
  },
  "mixins": [
    "vanillabackport.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.12",
    "platform": ">=1.2.7",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  }
}
